 <core:FragmentDefinition
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:u="sap.m.upload"
    xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:f="sap.f"
    xmlns:svm="sap.ui.comp.smartvariants">

    <f:DynamicPageHeader id="_IDGenDynamicPageHeader1">
                    <f:content>
                        <fb:FilterBar
                            id="_IDGenFilterBar1"
                            persistencyKey="myPersKey"
                            useToolbar="false"
                            search="fetchItems"
                            showClearOnFB="true"
                            clear="onClear"
                        >
                            <fb:filterGroupItems>
                                <fb:FilterGroupItem
                                    id="_IDGenFilterGroupItem2"
                                    groupName="G1"
                                    groupTitle="Group1"
                                    name="Customer Name"
                                    label="{i18n>Fcutname}"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <Input
                                            id="idcustomer"
                                            change="onchange"
                                            showValueHelp="false"
                                            valueHelpRequest="onCustomerHelpRequested"
                                            maxLength="40"
                                        />
                                    </fb:control>
                                </fb:FilterGroupItem>
                                <fb:FilterGroupItem
                                    id="_IDGenFilterGroupItem3"
                                    groupName="G1"
                                    groupTitle="Group1"
                                    name="Billing Name"
                                    label="{i18n>FMailingname}"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <Input
                                            id="idMailingname"
                                            showValueHelp="false"
                                            maxLength="40"
                                        />
                                    </fb:control>
                                </fb:FilterGroupItem>

                                <fb:FilterGroupItem
                                    id="_IDGenFilterGroupItem5"
                                    groupName="G2"
                                    groupTitle="Group2"
                                    name="Street Number"
                                    label="{i18n>FStreetNo}"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <Input
                                            id="idStreetNo"
                                            showValueHelp="false"
                                            maxLength="10"
                                        />
                                    </fb:control>
                                </fb:FilterGroupItem>
                                <fb:FilterGroupItem
                                    id="_IDGenFilterGroupItem4"
                                    groupName="G2"
                                    groupTitle="Group2"
                                    name="Street Name"
                                    label="{i18n>FStreetAdd2}"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <Input
                                            id="idStreetAdd"
                                            showValueHelp="false"
                                            maxLength="60"
                                            liveChange="onCapitalizeFirtsLater"
                                        />
                                    </fb:control>
                                </fb:FilterGroupItem>
                                <fb:FilterGroupItem
                                    id="_IDGenFilterGroupItem1"
                                    groupName="G2"
                                    groupTitle="Group2"
                                    name="City"
                                    label="{i18n>Fcity}"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <Input
                                            id="idCity"
                                            showValueHelp="false"
                                            maxLength="40"
                                            liveChange="onCapitalizeFirtsLater"
                                        />
                                    </fb:control>
                                </fb:FilterGroupItem>
                                <fb:FilterGroupItem
                                    id="_IDGenFilterGroupItem6"
                                    groupName="A"
                                    name="Zip Code"
                                    label="{i18n>FZipcod}"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <Input
                                            id="idzipcode"
                                            showValueHelp="false"
                                            maxLength="10"
                                        />
                                    </fb:control>
                                </fb:FilterGroupItem>
                                <fb:FilterGroupItem
                                    id="_IDGenFilterGroupItem7"
                                    groupName="A"
                                    name="No Of Line"
                                    label="{i18n>FNumberoflines}"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <Input
                                            id="idNoofline"
                                            showValueHelp="false"
                                            maxLength="10"
                                            type="Number"
                                        />
                                    </fb:control>
                                </fb:FilterGroupItem>
                                <fb:FilterGroupItem
                                    id="_IDGenFilterGroupItem8"
                                    groupName="A"
                                    name="Service Center"
                                    label="{i18n>Fsrvcenter}"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <Input
                                            id="idsrvcenter"
                                            showValueHelp="false"
                                            maxLength="65"
                                        />
                                    </fb:control>
                                </fb:FilterGroupItem>
                                <fb:FilterGroupItem
                                    id="_IDGenFilterGroupItem9"
                                    groupName="A"
                                    name="Cable No"
                                    label="{i18n>Fcableno}"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <Input
                                            id="idcableno"
                                            showValueHelp="false"
                                            maxLength="60"
                                        />
                                    </fb:control>
                                </fb:FilterGroupItem>
                                <fb:FilterGroupItem
                                    id="_IDGenFilterGroupItem10"
                                    groupName="A"
                                    name="PS Wiring Diagram"
                                    label="{i18n>Fpswidigram}"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <Input
                                            id="idPswdigram"
                                            showValueHelp="false"
                                            maxLength="225"
                                        />
                                    </fb:control>
                                </fb:FilterGroupItem>
                                <fb:FilterGroupItem
                                    id="_IDGenFilterGroupItem11"
                                    groupName="A"
                                    name="Primery Service Rep"
                                    label="{i18n>Fprimeryrvrep}"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <ComboBox
                                            id="idPrimarySRep"
                                            items="{path: 'dropDownJsonModel>/DropdownData', sorter: { path: 'device' }, filters: [
                                                {path: 'desc', operator: 'EQ', value1: 'PRIMARY SERVICE REP'}]}"
                                        >
                                            <core:Item
                                                id="_IDGenItem1"
                                                key="{dropDownJsonModel>value}"
                                                text="{dropDownJsonModel>value}"
                                            />
                                        </ComboBox>
                                    </fb:control>
                                </fb:FilterGroupItem>
                                <fb:FilterGroupItem
                                    id="_IDGenFilterGroupItem12"
                                    groupName="A"
                                    name="Account Rep"
                                    label="{i18n>Faccountrp}"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <Input
                                            id="idAcRep"
                                            showValueHelp="false"
                                            maxLength="40"
                                        />
                                    </fb:control>
                                </fb:FilterGroupItem>
                                <fb:FilterGroupItem
                                    id="_IDGenFilterGroupItem13"
                                    groupName="A"
                                    name="Substation"
                                    label="{i18n>Fsustatoin}"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <Input
                                            id="idSubstation"
                                            showValueHelp="false"
                                            maxLength="65"
                                        />
                                    </fb:control>
                                </fb:FilterGroupItem>
                                <fb:FilterGroupItem
                                    id="_IDGenFilterGroupItem14"
                                    groupName="A"
                                    name="Service Sketch No"
                                    label="{i18n>Fsrvsktchno}"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <Input
                                            id="idSrvSketchno"
                                            showValueHelp="false"
                                            maxLength="18"
                                        />
                                    </fb:control>
                                </fb:FilterGroupItem>
                                <fb:FilterGroupItem
                                    id="_IDGenFilterGroupItem15"
                                    groupName="A"
                                    name="Circuit"
                                    label="{i18n>Fcircuit}"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <Input
                                            id="idCircuit"
                                            showValueHelp="false"
                                            maxLength="225"
                                        />
                                    </fb:control>
                                </fb:FilterGroupItem>
                                <fb:FilterGroupItem
                                    id="_IDGenFilterGroupItemGeneration"
                                    groupName="A"
                                    name="On Site Generation"
                                    label="{i18n>FOnSiteGeneration}"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <ComboBox
                                            id="idOnSiteGeneration"
                                            items="{path: 'dropDownJsonModel>/DropdownData', sorter: { path: 'device' }, filters: [
                                                {path: 'field', operator: 'EQ', value1: 'ZPSO_CD_GENERATION'}]}"
                                        >
                                            <core:Item
                                                id="_IDGenItemGeneration"
                                                key="{dropDownJsonModel>value}"
                                                text="{dropDownJsonModel>value}"
                                            />
                                        </ComboBox>
                                    </fb:control>
                                </fb:FilterGroupItem>
                            </fb:filterGroupItems>
                        </fb:FilterBar>
                    </f:content>
                </f:DynamicPageHeader>
     
</core:FragmentDefinition>
